<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="MyMusicPage" prefHeight="115.0" prefWidth="410.0" style="-fx-background-color: #f4d6d6;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="MusicNameLabel" alignment="CENTER" prefHeight="59.0" prefWidth="410.0" text="MusicName">
         <font>
            <Font name="Century Gothic Bold Italic" size="18.0" />
         </font>
      </Label>
      <ProgressBar layoutY="59.0" prefHeight="9.0" prefWidth="410.0" progress="0.0" />
      <Button layoutY="76.0" mnemonicParsing="false" onAction="#PlayMusic" prefHeight="39.0" prefWidth="100.0" style="-fx-background-color: #f4d6d6; -fx-background-radius: 0;" text="Play">
         <font>
            <Font name="Century Gothic Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button layoutX="200.0" layoutY="76.0" mnemonicParsing="false" prefHeight="39.0" prefWidth="100.0" style="-fx-background-color: #f4d6d6; -fx-background-radius: 0;" text="Reset">
         <font>
            <Font name="Century Gothic Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button layoutX="100.0" layoutY="76.0" mnemonicParsing="false" onAction="#PauseMusic" prefHeight="39.0" prefWidth="100.0" style="-fx-background-color: #f4d6d6; -fx-background-radius: 0;" text="Pause">
         <font>
            <Font name="Century Gothic Bold Italic" size="12.0" />
         </font>
      </Button>
      <Slider layoutX="300.0" layoutY="76.0" prefHeight="39.0" prefWidth="110.0" />
   </children>
</AnchorPane>
