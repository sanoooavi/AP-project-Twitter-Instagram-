<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPage" prefHeight="100.0" prefWidth="420.0" style="-fx-background-color: #fbfcfc; -fx-border-color: #2A363F;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Circle fx:id="UserProfilePhoto" fill="#fbfcfc" layoutX="44.0" layoutY="48.0" radius="35.0" stroke="#2a363f" strokeType="INSIDE" />
      <Label fx:id="UsernameLabel" layoutX="94.0" layoutY="10.0" prefHeight="17.0" prefWidth="60.0" text="Username" textFill="#2a363f">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="messageLabel" layoutX="94.0" layoutY="35.0" text="last message" textFill="#2a363f">
         <font>
            <Font name="Century Gothic" size="12.0" />
         </font>
      </Label>
      <Button layoutX="96.0" layoutY="70.0" mnemonicParsing="false" onAction="#StartChat" prefHeight="25.0" prefWidth="283.0" style="-fx-background-color: #2A363F; -fx-border-radius: 15; -fx-background-radius: 15;" text="Start Chat" textFill="#fbfcfc">
         <font>
            <Font name="Century Gothic Bold Italic" size="12.0" />
         </font>
      </Button>
      <Circle fx:id="UserProfilePhoto1" fill="#2a363f" layoutX="390.0" layoutY="43.0" radius="11.0" stroke="#2a363f" strokeType="INSIDE" />
